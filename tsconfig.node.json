{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo.node",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "lib": [
      "ES2022",
      "DOM"
    ],
    "types": [
      "node"
    ],
    "noEmit": true
  },
  "include": [
    "vite.config.ts"
  ]
}
